    <!-- NOTICE SECTION JS -->
    <script>
      document.addEventListener("DOMContentLoaded", () => {
        loadNotices();
      });

      const notices = [
        {
          title: "Exam Schedule Released",
          description:
            "Semester exams start from April 15th. Check official site for details.",
          category: "Academic",
          date: "2025-04-01",
        },
        {
          title: "Hackathon Event",
          description:
            "Join the college Hackathon on April 10th. Prizes for winners!",
          category: "Events",
          date: "2025-04-02",
        },
        {
          title: "Placement Drive",
          description:
            "TCS and Infosys are conducting placement drives next week. Prepare accordingly.",
          category: "Placements",
          date: "2025-04-03",
        },
        {
          title: "Holiday Notice",
          description:
            "The university will remain closed on April 14th for the festival celebration.",
          category: "Other",
          date: "2025-04-04",
        },
      ];

      function loadNotices() {
        const container = document.getElementById("notices-container");
        container.innerHTML = "";

        notices.forEach((notice, index) => {
          setTimeout(() => {
            const card = document.createElement("div");
            card.className = "notice-card";
            card.dataset.category = notice.category;
            card.innerHTML = `
                <h3>üìå ${notice.title}</h3>
                <p>${notice.description}</p>
                <p><small>üìÖ ${notice.date} | üè∑ ${notice.category}</small></p>
            `;
            container.appendChild(card);
          }, index * 100);
        });
      }

      document.getElementById("search").addEventListener("keyup", function () {
        let filter = this.value.toLowerCase();
        document.querySelectorAll(".notice-card").forEach((card) => {
          let text = card.textContent.toLowerCase();
          card.style.display = text.includes(filter) ? "block" : "none";
        });
      });

      function filterNotices() {
        let filter = document.getElementById("categoryFilter").value;
        document.querySelectorAll(".notice-card").forEach((card) => {
          card.style.display =
            !filter || card.dataset.category === filter ? "block" : "none";
        });
      }
    </script>

    

